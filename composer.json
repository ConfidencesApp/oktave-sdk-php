{
    "name": "confidencesapp/oktave-sdk-php",
    "description": "The Oktave PHP SDK",
    "authors": [
        {
            "name": "oktave",
            "email": "support@oktave.com",
            "homepage": "https://www.oktave.com",
            "role": "Vendor"
        }
    ],
    "support": {
        "email": "support@oktave.co",
        "issues": "http://github.com/confidencesapp/oktave-php-sdk/issues",
        "source": "http://github.com/confidencesapp/oktave-php-sdk"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "7.*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "vlucas/phpdotenv": "^3.6",
        "pear/console_table": "^1.3.0",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Oktave\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Oktave\\SDK\\Tests\\" : "tests/"
        }
    }
}
